{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --force",
  "outputDirectory": "dist/browser",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
