{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --legacy-peer-deps --force",
  "outputDirectory": "dist/FrontSlack/browser",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/FrontSlack/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
